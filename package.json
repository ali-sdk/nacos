  "homepage": "https://github.com/ali-sdk/nacos#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "ci": {
    "type": "travis",
    "version": "8, 10"
  },
  "dependencies": {

  },
  "devDependencies": {
    "autod": "^3.0.1",
    "contributors": "^0.5.1",
